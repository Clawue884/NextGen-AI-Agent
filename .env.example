Database Configuration
POSTGRES_USER=your_postgres_user       # Ganti dengan nama pengguna database
POSTGRES_PASSWORD=your_postgres_password # Ganti dengan password database
POSTGRES_DB=your_postgres_db           # Ganti dengan nama database
DB_HOST=database                      # Hostname untuk database (default: database container name)
DB_PORT=5432                          # Port default PostgreSQL

# AWS S3 Configuration (Production)
AWS_ACCESS_KEY_ID=your_aws_access_key       # Ganti dengan AWS Access Key ID
AWS_SECRET_ACCESS_KEY=your_aws_secret_key   # Ganti dengan AWS Secret Access Key
AWS_REGION=us-east-1                        # Ganti dengan region AWS Anda
S3_BUCKET_NAME=your_s3_bucket_name          # Ganti dengan nama bucket S3 Anda

# MinIO Configuration (Development)
MINIO_ROOT_USER=minioadmin             # Ganti dengan user admin MinIO
MINIO_ROOT_PASSWORD=minioadminpassword # Ganti dengan password admin MinIO
MINIO_HOST=http://localhost:9000       # URL MinIO untuk pengembangan

# Backend Configuration
NODE_ENV=production                    # Mode backend (production/development)
PORT=5000                              # Port untuk layanan backend

# Frontend Configuration
REACT_APP_API_URL=http://localhost:5000 # URL backend untuk aplikasi frontend

# Security
JWT_SECRET=your_jwt_secret             # Secret key untuk JSON Web Token
ENCRYPTION_KEY=your_encryption_key     # Kunci untuk enkripsi data

# Additional Configurations
APP_NAME=YourAppName                   # Nama aplikasi
APP_URL=http://localhost               # URL utama aplikasi
DEBUG=false                            # Mode debug (true/false)


